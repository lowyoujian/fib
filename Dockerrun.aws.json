{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "aiscream/multi-client:latest",
            "hostname": "client",
            "essential": false,
            "memory": 128
        },
        
        {
            "name": "server",
            "image": "aiscream/multi-server:latest",
            "hostname": "api",
            "essential": false,
            "memory": 128
        },
        {
            "name": "worker",
            "image": "aiscream/multi-worker:latest", 
            "hostname": "worker",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "aiscream/multi-nginx:latest",
            "essential": true,
            "memory": 128,
            "portMapings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": [
                "client", "server"
            ]
        }
       
    ]
}